<div class="row">
  <div class="col s12">
    <h1>{{model.title}}</h1>
    <p>{{model.description}}</p>
    Data Points:
    <ul class="collection">
      {{#each model.dataPoints as |dataPoint|}}
        <li class="collection-item">{{dataPoint.name}}</li>
      {{/each}}
    </ul>
    Data Collections:
    <ul class="collection">
      {{#each model.dataCollections as |dataCollection|}}
        {{#each dataCollection.dataPoints as |dataPoint|}}
          {{dataPoint.name}}
        {{/each}}
        <li class="collection-item">{{dataCollection.title}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="col s12">
    {{!-- {{profiles-list-modal profiles=this.profiles model=this.model}} --}}
    <a {{action "saveReport" model}} href="#!" class="modal-action modal-close waves-effect waves-green btn">Add to Profile</a>
  </div>
</div>